<%= form_with model: @profile do |form| %>
  <%= form.label 'name' %><br>
  <%= form.text_field :name %><br>
  <%= form.label 'age' %><br>
  <%= form.text_field :age %><br>
  <%= form.label 'username' %><br>
  <%= form.text_field :username %><br>
  <%= form.label 'city' %><br>
  <%= form.text_field :city %><br>
  <%= form.label 'state' %><br>
  <%= form.text_field :state %><br>
  <%= form.submit 'Salvar' %>
<% end %>
